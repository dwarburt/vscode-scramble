{
	"name": "scramble",
	"displayName": "scramble",
	"description": "scramble highlighted text while maintaining structure",
	"publisher": "rubbersoft",
	"repository": {
		"type": "git",
		"url": "https://github.com/dwarburt/vscode-scramble"
	},
	"type": "commonjs",
	"keywords": [
		"anonymous",
		"anonymize",
		"scramble",
		"random",
		"randomize"
	],
	"icon": "images/iconxcf.png",
	"license": "MIT",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:scramble.scramble"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "scramble.scramble",
				"title": "Scramble text"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.1",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"eslint": "^7.20.0",
		"glob": "^7.1.6",
		"mocha": "^8.3.0",
		"typescript": "^4.2.2",
		"vscode-test": "^1.5.1"
	}
}
